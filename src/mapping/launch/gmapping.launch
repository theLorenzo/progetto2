
<launch>


    <node pkg="tf" type="static_transform_publisher" name="odom_camera_odom_frame" args="0 0 0 0 0 0 1 odom camera_odom_frame 1000" />
    <node pkg="tf" type="static_transform_publisher" name="camera_pose_frame_camera_gyro_frame" args="0 0 0 0 0 0 1 camera_pose_frame camera_gyro_frame 1000" />
    <node pkg="tf" type="static_transform_publisher" name="camera_pose_frame_camera_accel_frame" args="0 0 0 0 0 0 1 camera_pose_frame camera_accel_frame 1000" />
    <node pkg="tf" type="static_transform_publisher" name="camera_pose_frame_camera_gyro_optical" args="0 0 0 0 0 0 1 camera_pose_frame camera_gyro_optical_frame 1000" />
    <node pkg="tf" type="static_transform_publisher" name="camera_pose_frame_camera_accel_optical" args="0 0 0 0 0 0 1 camera_pose_frame camera_accel_optical_frame 1000" />




    <node pkg="rqt_tf_tree" type = "rqt_tf_tree" name = "rqt_tf_tree"/>
    <node type="rviz" name="rviz_comparazione" pkg="rviz" args="-d $(find mapping)/config/config_rviz.rviz"/>

</launch>
